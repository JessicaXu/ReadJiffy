cp /opt/git/app_style/values/* values/
cp /opt/git/app_style/drawable/* drawable/
cp /opt/git/app_style/drawable-hdpi/* drawable-hdpi/
cp /opt/git/app_style/drawable-mdpi/* drawable-mdpi/
cp /opt/git/app_style/drawable-xhdpi/* drawable-xhdpi/
cp /opt/git/app_style/drawable-xxhdpi/* drawable-xxhdpi/
done
